int windowX = 255;
int windowY = 255;
int[] r = new int[windowX];
int[] g = new int[windowX];
int[] b = new int[windowX];
int r_c = 255;
int g_c = 0;
int b_c = 0;
int counter = 1;
int i = 0;

void setup(){
  background(0,0,0);
  size(windowX, windowY);
  noStroke();
  frameRate(60);

} // END SETUP

void draw(){
       if (r_c == 255){
         g_c += 1;
       } else if(g_c == 255 && ){
         r_c -= 1;
       } else if(r_c == 0){
         b_c += 1;
       } else if(b_c == 255){
         g_c -= 1;
       }
       fill(r_c,g_c,b_c);
       rect(1,1,windowX,windowY);
       counter++;

} // END DRAW
